	  	});
	});
	
	function advancedSearch() {
        if(true){
            window.location.href = "/epps/prepareAdvancedSearch.do?type=cftFTS";
        } else {
            window.location.href = "/epps/prepareAdvancedSearch.do?type=cft";
        }
    }

</script>




